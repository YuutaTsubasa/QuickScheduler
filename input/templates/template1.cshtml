Title: "初始版本"
Author: "悠太翼"
Thumbnail: "/images/weekly/template1.webp"
OutsidePageContent: true
TemplateCssFile: "/styles/templates/templates1.css"
Published: 2023/04/14
---
@Html.Partial("_ButtonList")

<section class="weekly_container">
    <section class="weekly">
        <section class="weekly_left_part">
            <div class="weekly_author_part"></div>
        </section>
        <section class="weekly_right_part">
            <div class="weekly_title_group"><div class="channel_logo_part"></div><h1 class="weekly_title">本週行事曆</h1></div>
            <ul class="weekly_items">
                @foreach(var index in Enumerable.Range(0, 7)){
                    <li class="weekly_item">
                    <div class="weekly_item_date"><span class="content_part"><span class="date_part">00/00</span><span class="time_group_part"><span class="weekday_part"></span> <span class="time_part"></span></span></span></div>
                        <div class="content stream_off">
                            <ul class="weekly_tags"></ul>
                            <pre class="weekly_day_title"></pre>
                        </div>
                    </li>
                }
            </ul>
            <nav class="weekly_community">
                <ul>

                </ul>
            </nav>
        </section>
    </section>
</section>

@Html.Partial("_DataForm")

<script src='@Context.GetLink("/scripts/weekly.js")'></script>